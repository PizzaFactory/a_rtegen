[template private genBswSchedulableEntity (aBswSchedulableEntity : BswSchedulableEntity) /]

[template private genBswSchedulableEntity (aBswSchedulableEntity : ComSendSignalPeriodicEntity) ]
[aBswSchedulableEntity.returnType.symbolName/]
[aBswSchedulableEntity.symbolName/]([aBswSchedulableEntity.genFunctionParams()/])
{
	[aBswSchedulableEntity.genLocalVariableDeclarations()/]
	[for (aProxyOperation : ComSendProxyOperation | aBswSchedulableEntity.operation)]
		while (IOC_E_NO_DATA != [aProxyOperation.accessApi.symbolName/]([aProxyOperation.signalIdVariable.genPointerAccess()/], [aProxyOperation.readValueVariable.genPointerAccess()/])) {
			(void) Com_SendSignal([aProxyOperation.signalIdVariable.genValueAccess()/], [aProxyOperation.readValueVariable.genPointerAccess()/]);
		}
	[/for]
}
[/template]

[template private genBswSchedulableEntity (aBswSchedulableEntity : ComSendSignalImmediateEntity) ]
[aBswSchedulableEntity.returnType.symbolName/]
[aBswSchedulableEntity.symbolName/]([aBswSchedulableEntity.genFunctionParams()/])
{
	[aBswSchedulableEntity.genLocalVariableDeclarations()/]
	[for (aProxyOperation : ComSendProxyOperation | aBswSchedulableEntity.operation)]
	    while (IOC_E_NO_DATA != [aProxyOperation.accessApi.symbolName/]([aProxyOperation.signalIdVariable.genPointerAccess()/], [aProxyOperation.readValueVariable.genPointerAccess()/])) {
	        (void) Com_SendSignal([aProxyOperation.signalIdVariable.genValueAccess()/], [aProxyOperation.readValueVariable.genPointerAccess()/]);
	    }
	[/for]
}
[/template]
