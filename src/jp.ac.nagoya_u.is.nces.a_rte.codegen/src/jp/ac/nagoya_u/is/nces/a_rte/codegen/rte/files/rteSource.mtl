[template private genRteApi(aRteApi : CallApi) ? (aRteApi.isConnected)]
[aRteApi.returnType.symbolName/]
[aRteApi.symbolName/]([aRteApi.genFunctionParams()/]) /* MISRA RULE 8.8 VIOLATION : It won't be fixed because of RTE Specification rte_sws_1006 */
{
	[aRteApi.genLocalVariableDeclarations()/]

	[for (aExcludeOperation : ExcludeOperation | aRteApi.operation.excludeOperation)]
		[aExcludeOperation.genExcludeOperationEnter()/]
	[/for]

	[let paramValues : Sequence(Value) = aRteApi.portArgValue.oclAsType(Value)->addAll(aRteApi.operationParam)]
		[if (aRteApi.returnVariable.oclIsUndefined())]
			[aRteApi.operation.startExecutable.symbolName/]([paramValues.genSymbolAccess()->sep(', ')/]);
		[else]
			[aRteApi.returnVariable.genValueAccess()/] = [aRteApi.operation.startExecutable.symbolName/]([paramValues.genSymbolAccess()->sep(', ')/]);
		[/if]
	[/let]
	
	[for (aExcludeOperation : ExcludeOperation | aRteApi.operation.excludeOperation->reverse())]
		[aExcludeOperation.genExcludeOperationExit()/]
	[/for]
	return([aRteApi.returnValue.genValueAccess()/]);
}
[/template]

