	[aGroup.memoryMapping.genMemoryMappingStart()/]

	[for (aComCallback : ComCallback | aGroup.comCallback)]
		[aComCallback.genComCallback()/]

	[/for]

	[aGroup.memoryMapping.genMemoryMappingEnd()/]

[/for]
